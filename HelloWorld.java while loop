public class HelloWorld {
  public static void main(String[] args) {
  
    int i = 2;
    while (i < 8){
     if(i == 8){
       break;
     }
     System.out.println(i);
     System.out.println(0);
     i++;
    
    
     
     
     
     
     
     //Nested

    }//While
  
  }//main
  
}//class